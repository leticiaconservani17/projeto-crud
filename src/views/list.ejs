<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head.ejs') %>
</head>
<body>
    <h1> <%= title %> </h1>

    <%- include('partials/navbar.ejs') %>

    <% users.forEach((user) => { %>
